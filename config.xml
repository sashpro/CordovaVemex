<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.vemex" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Vemex</name>
    <description>
        Crypto currency exchange
    </description>
    <author email="support@vemex.net" href="http://vemex.net">
        Vemex Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="https://vemex.net/*" />
    <allow-navigation href="https://advcash.com/*" />
    <allow-navigation href="https://google.com/*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-crosswalk-webview" spec="~2.3.0" />
</widget>
